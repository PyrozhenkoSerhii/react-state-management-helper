{
  "name": "state-management-helper",
  "version": "1.0.0",
  "description": "An app to help developers to choose right state management approach for their current project",
  "main": "index.js",
  "scripts": {
    "client-build": "webpack --config webpack.config.prod.js --mode production",
    "client-dev-redux-v1": "webpack-dev-server --config webpack.config.dev.js  --env.project=redux/v1",
    "client-dev-redux-v2": "webpack-dev-server --config webpack.config.dev.js --env.project=redux/v2",
    "client-dev-mobx-v1": "webpack-dev-server --config webpack.config.dev.js  --env.project=mobx/v1",
    "client-dev-context-v1": "webpack-dev-server --config webpack.config.dev.js --env.project=context/v1",
    "client-dev-presentation": "webpack-dev-server --config webpack.config.dev.js --env.project=presentation --env.port=81",
    "server-dev": "nodemon -r dotenv/config ./server/index.ts",
    "server-build": "tsc --project ./server/",
    "lint-client": "eslint src",
    "db-populate": "npx ts-node -r dotenv/config ./server/utils/populate.ts",
    "db-clear-tracker": "npx ts-node -r dotenv/config ./server/utils/clearTracker.ts"
  },
  "keywords": [],
  "author": "Serhii Pyrozhenko",
  "license": "",
