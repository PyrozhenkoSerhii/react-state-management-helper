{
  "name": "state-management-helper",
  "version": "1.0.0",
  "description": "An app to help developers to choose right state management approach for their current project",
  "main": "index.js",
  "scripts": {
    "project-init": "npm i && cd src/redux && npm i && cd ../mobx && npm i",
    "client-build": "webpack --config webpack.config.prod.js --mode production",
    "client-dev-redux-v1": "webpack-dev-server --config webpack.config.dev.js --env.type=redux --env.version=v1",
    "client-dev-redux-v2": "webpack-dev-server --config webpack.config.dev.js --env.type=redux --env.version=v2",
    "client-dev-mobx-v1": "webpack-dev-server --config webpack.config.dev.js --env.type=mobx --env.version=v1",
    "client-dev-context-v1": "webpack-dev-server --config webpack.config.dev.js --env.type=context --env.version=v1",
    "server-dev": "nodemon -r dotenv/config --exec babel-node ./server/index.js",
    "lint": "eslint src && eslint server"
  },
  "keywords": [],
  "author": "Serhii Pyrozhenko",
  "license": "",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "better-npm-run": "^0.1.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.22",
    "mongoose-timestamp": "^0.6.0",
    "nodemon": "^2.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2"
  }
}
